
#1.) $> sudo pysetup install apscheduler
tar -zxvf APScheduler-2.1.1.tar.gz
cd APScheduler-2.1.1
python setup.py install

#2.) install xe-0.7.4:
tar -xvzf xe-0.7.4.tar.gz; \
cd xe-0.7.4; \
chmod 755 setup.py; \
./setup.py build; \
./setup.py install

#3.) install pyfeed-0.7.4:
tar -xvzf pyfeed-0.7.4.tar.gz; \
cd pyfeed-0.7.4; \
chmod 755 setup.py; \
./setup.py build; \
./setup.py install

#4.) install gdata:
tar -xvzf gdata-2.0.18.tar.gz; \
cd gdata-2.0.18; \
chmod 755 setup.py; \
./setup.py build; \
./setup.py install; \
./tests/run_data_tests.py
